                  <inertial pos="-0.0068528 -3.9973e-06 0.039705" quat="0.494949 0.495288 -0.504676 0.504992" mass="0.077892" diaginertia="4.8843e-05 3.8232e-05 1.7707e-05"/>
                  <joint name="joint6" pos="0 0 0" axis="0 0 -1" range="-2.35 2.35"/>
                  <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="1 1 1 1" mesh="Link6"/>
                  <geom type="mesh" rgba="1 1 1 1" mesh="Link6"/>
                  <body name="Link7_1" pos="-0.0056012 -0.029636 0.0706" quat="0.499848 -0.50015 -0.49985 -0.500152">
                    <inertial pos="0.018927 0.006 0.012082" quat="0 0.677073 0 0.735916" mass="0.015046" diaginertia="2.62715e-06 2.0229e-06 1.25975e-06"/>
                    <joint name="gripper1" pos="0 0 0" axis="0 0 -1" type="slide" range="0 0.03"/>
                    <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="1 1 1 1" mesh="Link7_1"/>
                    <geom type="mesh" rgba="1 1 1 1" mesh="Link7_1"/>
                  </body>
                  <body name="Link7_2" pos="-0.0056388 0.02964 0.0706" quat="0.500148 0.49985 -0.50015 0.499852">
                    <inertial pos="0.018927 -0.006 0.012082" quat="0 0.677073 0 0.735916" mass="0.015046" diaginertia="2.62715e-06 2.0229e-06 1.25975e-06"/>
                    <joint name="gripper2" pos="0 0 0" axis="0 0 1" type="slide" range="-0.03 0"/>
                    <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="1 1 1 1" mesh="Link7_2"/>
                    <geom type="mesh" rgba="1 1 1 1" mesh="Link7_2"/>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <!-- Switch to position actuators -->
  <actuator>
    <position class="d1" name="motor1" joint="joint1" ctrlrange="-2.35 2.35" kp="10000" kv="10"/>
    <position class="d1" name="motor2" joint="joint2" ctrlrange="-1.57 1.57" kp="10" kv="10"/>
    <position class="d1" name="motor3" joint="joint3" ctrlrange="-1.57 1.57" kp="10" kv="10"/>
    <position class="d1" name="motor4" joint="joint4" ctrlrange="-2.35 2.35" kp="1000" kv="10"/>
    <position class="d1" name="motor5" joint="joint5" ctrlrange="-1.57 1.57" kp="10000" kv="10"/>
    <position class="d1" name="motor6" joint="joint6" ctrlrange="-2.35 2.35" kp="1000" kv="10"/>
    <position class="d1" name="motor7_1" joint="gripper1" ctrlrange="0 0.03" kp="1000" kv="10"/>
    <position class="d1" name="motor7_2" joint="gripper2" ctrlrange="-0.03 0" kp="1000" kv="10"/>
  </actuator>

  <sensor>
    <jointpos name="joint1_sensor" joint="joint1"/>
    <jointpos name="joint2_sensor" joint="joint2"/>
    <jointpos name="joint3_sensor" joint="joint3"/>
    <jointpos name="joint4_sensor" joint="joint4"/>
    <jointpos name="joint5_sensor" joint="joint5"/>
    <jointpos name="joint6_sensor" joint="joint6"/>
    <jointpos name="gripper1_sensor" joint="gripper1"/>
    <jointpos name="gripper2_sensor" joint="gripper2"/>
  </sensor>
  <keyframe>
    <key name="zero" qpos="0 0 0 0 0 0 0 0" ctrl="0 0 0 0 0 0 0 0"/>
    <key name="home" qpos="0 -1.57 1.57 0 0 0 0 0.03" ctrl="0 -1.57 1.57 0 0 0 0 0"/>
  </keyframe>
</mujoco>
